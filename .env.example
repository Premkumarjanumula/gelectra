# This file contains all the environment variables needed to run the application.
# Copy this file to a new file named .env and fill in the values.

# ---------------------------------
# FIREBASE CONFIGURATION
# ---------------------------------
# You can find these values in your Firebase project settings.
# Go to Project Settings > General > Your apps > Web app > SDK setup and configuration > Config
NEXT_PUBLIC_FIREBASE_API_KEY="YOUR_API_KEY"
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN="YOUR_AUTH_DOMAIN"
NEXT_PUBLIC_FIREBASE_PROJECT_ID="YOUR_PROJECT_ID"
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET="YOUR_STORAGE_BUCKET"
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID="YOUR_MESSAGING_SENDER_ID"
NEXT_PUBLIC_FIREBASE_APP_ID="YOUR_APP_ID"

# ---------------------------------
# RESEND API KEY
# ---------------------------------
# This is used for the contact form to send emails.
# You can get your key from the Resend dashboard: https://resend.com/
RESEND_API_KEY="YOUR_RESEND_API_KEY"
